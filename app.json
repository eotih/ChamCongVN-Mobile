{
  "expo": {
    "plugins": [
      [
        "expo-notifications",
        {
          "sounds": [
            "./assets/soundthongbao.wav"
          ]
        }
      ]
    ],
    "name": "Chấm công VN",
    "slug": "Chấm công VN",
    "privacy": "public",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.7.1",
    "orientation": "portrait",
    "icon": "./assets/icon1.png",
    "splash": {
      "image": "./assets/Loading.png",
